{
  "keyboard_name": "D1P",
  "maintainer": "PlipWorks",
  "manufacturer": "PLIP Works",
  "url": "https://plip.works",
  "bootloader_instructions": "Hold down the key while plugging in the keyboard.",
  "usb": {
    "vid": "0x5057",
    "pid": "0x0001",  
    "device_version": "1.0.0",
    "force_nkro": true,
    "max_power": 100
  },
  "processor": "atmega32u2",
  "bootloader": "atmel-dfu",
  "build": {
    "firmware_format": "hex",
    "lto": true
  },
  "bootmagic": {
    "enabled": true,
    "matrix": [0, 0]
  },

  "layouts": {
    "LAYOUT": {
      "layout": [{
          "x":0,
          "y":0,
          "matrix": [0, 0]
      }]
    }
  },

  "matrix_pins": {
    "cols": ["B7"],
    "rows": ["D2"]
  },
  "debounce": 5,
  "diode_direction": "COL2ROW",

  "rgblight": {
    "led_count": 2,
    "animations": {
      "breathing": true,
      "rainbow_mood": true,
      "rainbow_swirl": true
    }
  }
}




